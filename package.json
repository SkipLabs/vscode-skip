{
  "name": "skip",
  "displayName": "SkipLang",
  "publisher": "skiplabs",
  "description": "Manage Skip Language",
  "version": "0.0.1",
  "readme": "README.md",
  "author": {
    "name": "SkipLabs"
  },
  "engines": {
    "vscode": "^1.41.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SkipLabs/skdb.git",
    "directory": "ide/vscode/skip"
  },
  "license": "MIT",
  "keywords": [
    "Skip",
    "SkipLabs",
    "SkipLang",
    "SKDB"
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "breakpoints": [
      {
        "language": "skip"
      }
    ],
    "languages": [
      {
        "id": "skip",
        "aliases": [
          "Skip",
          "skip"
        ],
        "extensions": [
          ".sk"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "skip",
        "scopeName": "source.skip",
        "path": "./syntaxes/skip.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "fmt": "npx prettier --write ."
  },
  "devDependencies": {
    "prettier": "^3.0.3"
  }
}
